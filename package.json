{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev":"next dev && sass --watch --update --style=expanded styles/scss:styles/css",
    "build":"npm run build:scss && next build",
    "start":"next start",
    "lint":"next lint",
    "build:scss": "sass styles/scss -o styles/css"
  },
  "dependencies": {
    "next": "14.2.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "sass": "^1.77.6"
  }
}
